/*
// Vector 
// If I want to use #include <iostream> and then <vector> for vector library . 
// So, it must be helpful for me in future interview.
#include<iostream>
// A vector is a dynnamic array ;
#include<vector>
using namespace std ;
int main(){
    // Declear a vector 
    vector<int> v ;
    // This creates an empty vector of integers .
    // push_back(insert element)

    v.push_back(10);
    v.push_back(20);
    v.push_back(30);
    // (.size)Tells How many elements exist
    cout<<v.size()<<endl;
    // Index Access 
    
    cout <<v[0]<<" ";//First Element 
    cout<<v[1]<<" ";// Second 
    cout<<v[2]<<" ";//third
    
    // Method -1 Loop Traversal
    for(int i=0;i<v.size();i++){
        cout<<v[i]<<" ";
    }
       
    // Method -2: Range Loop(mordern C++)
    for(int x:v){
        cout<<x<<" ";
    }
         
    // 6. Copy Vector 
    vector<int> copy= v ;
    // Vector support direct copy 
    copy[0]=100;
    cout<<v[0]<<endl;
    cout<<copy[0];
    
    // 7. 2D vector 
    // Vector inside vector=matrix 
    vector<vector<int>>matrix;
    matrix.push_back({1,2,3,});
    matrix.push_back({4,5,6});
    
    cout<<"Matrix element: "<<matrix[1][2]<<endl;
    // If we want to see exact values inside 2-d matrix 
    // We simply apply a nested loop
    for(int i=0;i<matrix.size();i++){
        for(int j=0;j<matrix[i].size();j++){
            cout<<matrix[i][j]<<" ";
        }
        cout<<endl;
    }
    return 0 ;
}
// Stack
// Last IN First Out 
#include<iostream>
// For stack library
#include<stack>
using namespace std ;
int main(){
    // Decleared Stack
    stack<int> st;
    // Add Element 
    st.push(10);
    st.push(20);
    st.push(30);
    // top = last element (For easy remember)
    cout<<st.top()<<endl;
    // Size = How many elements belongs in stack
    cout<<st.size()<<endl;
    // Check if stack is empty  using if
    if(st.empty()) cout<<"Empty";
    // Operation-> Delete top -> delete last element 
    st.pop();
    // Another way usig while loop
    // Reverse Stack 
    while(!st.empty()){
        cout<<st.top()<<" ";
        st.pop();
    }
}*/
//Queue 
// First come first service 
#include<iostream>
//Queue
#include<queue>
using namespace std;
int main(){
    queue<int> q ;
    //Insert Element 
    q.push(10);
    q.push(20);
    q.push(30);
    // Front -> see first Element 
    cout<<q.front()<<endl;// just show first element 
    //q.pop();// Delete first element 
    if(q.empty())cout<<"Empty";
    // Traversal;
    while(!q.empty()){
        cout<<q.front()<<" ";
        q.pop();
    }
    
}



